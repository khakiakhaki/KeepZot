{
    "manifest_version": 2,
    "name": "KeepZot",
    "version": "0.2",
    "description": "Minimize zotero to tray for zotero 7",
    "homepage_url": "https://localhost:8000",
    "applications": {
        "zotero": {
            "id": "keepzot@kk",
            "update_url": "https://localhost:8000/keepzot_update.json",
            "strict_min_version": "6.999",
            "strict_max_version": "7.0.*"
        }
    }
}
